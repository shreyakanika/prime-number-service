    <modelVersion>4.0.0</modelVersion>
    <groupId>com.natwest</groupId>
    <artifactId>prime-number-service</artifactId>
    <version>1.0-SNAPSHOT</version>
    <name>prime-number-service</name>
    <packaging>pom</packaging>
    <properties>
